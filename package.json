{
  "name": "tp-admin-ui",
  "version": "1.16.0",
  "description": "Manage all your travel agent needs at one place",
  "private": true,
  "bugs": {
    "url": "https://github.com/tourepedia/tp-admin-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tourepedia/tp-admin-ui.git"
  },
  "dependencies": {
    "@reach/router": "1.2.1",
    "@tourepedia/css": "0.11.0",
    "@tourepedia/ui": "1.3.8",
    "@types/classnames": "2.2.7",
    "@types/jest": "24.0.5",
    "@types/node": "11.9.4",
    "@types/pluralize": "0.0.29",
    "@types/pusher-js": "4.2.2",
    "@types/qs": "6.5.1",
    "@types/reach__router": "1.2.3",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.1.1",
    "@types/yup": "0.26.9",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "formik": "1.5.1",
    "laravel-echo": "1.5.4",
    "moment": "2.24.0",
    "pluralize": "8.0.0",
    "pusher-js": "5.0.0",
    "qs": "6.6.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet-async": "0.2.0",
    "react-redux": "7.1.0",
    "react-scripts": "3.0.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "typesafe-actions": "4.4.0",
    "typescript": "3.5.1",
    "utility-types": "3.4.1",
    "yup": "0.26.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "cypress": "cypress open",
    "cypress:all": "cypress run",
    "commit": "git-cz",
    "release": "env-cmd -f .env.tokens semantic-release",
    "postrelease": "npm run deploy:production",
    "build": "react-scripts build",
    "deploy": "gh-pages-clean && gh-pages -d build -t",
    "predeploy:testing": "env-cmd -f ./.env.test npm run build",
    "predeploy:production": "env-cmd -f ./.env.production npm run build",
    "deploy:testing": "npm run deploy -- -b gh-pages__test",
    "deploy:production": "npm run deploy -- -b gh-pages__production"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@cypress/webpack-preprocessor": "4.0.3",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "@testing-library/cypress": "4.0.4",
    "@types/faker": "4.1.5",
    "commitizen": "3.1.1",
    "cross-env": "5.2.0",
    "cypress": "3.3.2",
    "cz-conventional-changelog": "2.1.0",
    "env-cmd": "9.0.1",
    "eslint-plugin-cypress": "2.2.1",
    "faker": "4.1.0",
    "gh-pages": "2.0.1",
    "husky": "2.4.1",
    "lint-staged": "8.2.0",
    "prettier": "1.16.4",
    "redux-devtools-extension": "2.13.8",
    "semantic-release": "17.2.3"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "cross-env CI=true npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,html,css,scss,sass,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} :tada: :rocket:\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ],
    "ci": false
  }
}
